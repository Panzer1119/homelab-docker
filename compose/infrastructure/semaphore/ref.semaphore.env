# General
TZ="UTC"
SEMAPHORE_ACCESS_KEY_ENCRYPTION="op://Docker/Semaphore/Access-Key-Encryption"
SEMAPHORE_WEB_HOST="op://Docker/Semaphore/URL"

# Database
SEMAPHORE_DB_DIALECT="postgres"
SEMAPHORE_DB_HOST="postgres"
SEMAPHORE_DB_PORT="5432"
SEMAPHORE_DB="semaphore"
SEMAPHORE_DB_USER="op://Docker/Semaphore/Database/Username"
SEMAPHORE_DB_PASS="op://Docker/Semaphore/Database/Password"

# Ansible
SEMAPHORE_PLAYBOOK_PATH="/tmp/semaphore/"
#ANSIBLE_HOST_KEY_CHECKING="true" # (optional) change to true if you want to enable host key checking

# Admin user
SEMAPHORE_ADMIN="op://Docker/Semaphore/Username"
SEMAPHORE_ADMIN_PASSWORD="op://Docker/Semaphore/Password"
SEMAPHORE_ADMIN_EMAIL="op://Docker/Semaphore/Email"
SEMAPHORE_ADMIN_NAME="op://Docker/Semaphore/Name"
