# A full list of options can be found in the '.env.example' file.

#APP_NAME="Speedtest Tracker"
#APP_ENV=production
#APP_DEBUG=false

# Application key
# Used for encryption where needed, if a key is not generated run: php artisan key:generate
APP_KEY="op://Docker/Speedtest Tracker/App-Key"

# Application URL
# Not used in the URL but effects links in emails and notifications: https://docs.speedtest-tracker.dev/faqs#links-in-emails-dont-point-to-the-correct-url
APP_URL="op://Docker/Speedtest Tracker/App-URL"
ASSET_URL="op://Docker/Speedtest Tracker/Asset-URL"

# SMTP mail config
# Only SMTP protocol is supported at this time: https://docs.speedtest-tracker.dev/settings/notifications/mail
#MAIL_MAILER="smtp"
#MAIL_HOST="op://Docker/Speedtest Tracker/Mail/Host"
#MAIL_PORT="465"
#MAIL_USERNAME="op://Docker/Speedtest Tracker/Mail/Username"
#MAIL_PASSWORD="op://Docker/Speedtest Tracker/Mail/Password"
#MAIL_ENCRYPTION="tls"
#MAIL_FROM_ADDRESS="op://Docker/Speedtest Tracker/Mail/From-Address"
#MAIL_FROM_NAME="op://Docker/Speedtest Tracker/Mail/From-Name"

# Telegram bot config
#TELEGRAM_BOT_TOKEN="op://Docker/Speedtest Tracker/Telegram/Bot-Token"

#APP_TIMEZONE="Europe/Berlin"
DISPLAY_TIMEZONE="Europe/Berlin"

SPEEDTEST_SCHEDULE="5 * * * *"
#SPEEDTEST_SERVERS=""
PRUNE_RESULTS_OLDER_THAN="0"
